{
	"version": "0.8.4",
	"allow_edit": true,
	"plugins": [],
	"panes": [
		{
			"title": "Démarrage de l'application",
			"width": 1,
			"row": {
				"3": 1,
				"4": 1
			},
			"col": {
				"3": 2,
				"4": 1
			},
			"col_width": 1,
			"widgets": [
				{
					"type": "sshcommand",
					"settings": {
						"style": "switch",
						"host": "192.168.100.199",
						"login": "root",
						"password": "pcduino",
						"oncommand": "/root/programmes_python/startAsservissementVitesse.sh",
						"offcommand": "/root/programmes_python/stopPythonPWM.sh",
						"ontext": "ON",
						"offtext": "OFF",
						"initialstate": false,
						"caption": "Switch ON"
					}
				}
			]
		},
		{
			"title": "Consigne moteur",
			"width": 1,
			"row": {
				"3": 1,
				"4": 1,
				"5": 9
			},
			"col": {
				"3": 1,
				"4": 2,
				"5": 1
			},
			"col_width": 1,
			"widgets": [
				{
					"type": "switchbutton",
					"settings": {
						"title": "Type de signal (carré ou sinus)",
						"variable": "datasources[\"ws\"][\"typeSignal\"]",
						"yestext": "Sinus",
						"notext": "Carré",
						"initialstate": false
					}
				},
				{
					"type": "slider",
					"settings": {
						"title": "Offset",
						"variable": "datasources[\"ws\"][\"offset\"]",
						"formula": "x",
						"initialvalue": 0,
						"min": -80,
						"max": 80,
						"resolution": 1,
						"resetvalue": 0,
						"resetcaption": "Reset"
					}
				},
				{
					"type": "slider",
					"settings": {
						"title": "Amplitude",
						"variable": "datasources[\"ws\"][\"amplitude\"]",
						"formula": "x",
						"initialvalue": 0,
						"min": 0,
						"max": 80,
						"resolution": 1,
						"resetvalue": 0,
						"resetcaption": "Reset"
					}
				},
				{
					"type": "slider",
					"settings": {
						"title": "Fréquence",
						"variable": "datasources[\"ws\"][\"frequence\"]",
						"formula": "x",
						"initialvalue": 0,
						"min": 0,
						"max": 5,
						"resolution": 2,
						"resetvalue": 0,
						"resetcaption": "Reset"
					}
				}
			]
		},
		{
			"title": "Vitesse moteur",
			"width": 1,
			"row": {
				"3": 1,
				"4": 5,
				"5": 9
			},
			"col": {
				"3": 3,
				"4": 1,
				"5": 3
			},
			"col_width": 1,
			"widgets": [
				{
					"type": "text_widget",
					"settings": {
						"title": "Vitesse moteur",
						"size": "regular",
						"value": "datasources[\"ws\"][\"omega\"]",
						"animate": false,
						"units": "rad/s"
					}
				}
			]
		},
		{
			"title": "Tracés",
			"width": 1,
			"row": {
				"2": 9,
				"3": 5,
				"4": 1,
				"5": 7
			},
			"col": {
				"2": 2,
				"3": 2,
				"4": 3,
				"5": 3
			},
			"col_width": "2",
			"widgets": [
				{
					"type": "plot",
					"settings": {
						"title": "Commande et vitesse",
						"xaxis": "datasource_column",
						"time": "datasources[\"ws\"][\"Temps\"]",
						"time_window": "4",
						"value": [
							"datasources[\"ws\"][\"Consigne\"]",
							"datasources[\"ws\"][\"omega\"]"
						],
						"y_axis_min_range": "-1,1",
						"y2value": [
							"datasources[\"ws\"][\"commande\"]"
						],
						"y2_axis_min_range": "-10,10",
						"include_legend": true,
						"legendStr": "Ref,omega,commande",
						"height": 4,
						"pausable": true
					}
				}
			]
		},
		{
			"title": "Boucle fermée",
			"width": 1,
			"row": {
				"3": 15,
				"4": 11
			},
			"col": {
				"3": 2,
				"4": 3
			},
			"col_width": "2",
			"widgets": [
				{
					"type": "closedloop",
					"settings": {
						"title": " Asservissement de vitesse",
						"ref": "datasources[\"ws\"][\"Consigne\"]",
						"error": "(datasources[\"ws\"][\"Consigne\"]-datasources[\"ws\"][\"omega\"]).toFixed(2)",
						"command": "datasources[\"ws\"][\"commande\"]",
						"output": "datasources[\"ws\"][\"omega\"]"
					}
				}
			]
		},
		{
			"title": "Gains du PID",
			"width": 1,
			"row": {
				"3": 17,
				"4": 14,
				"5": 11
			},
			"col": {
				"3": 1,
				"4": 2,
				"5": 2
			},
			"col_width": 1,
			"widgets": [
				{
					"type": "slider",
					"settings": {
						"title": "Kp",
						"variable": "datasources[\"ws\"][\"Kp\"]",
						"formula": "x",
						"initialvalue": 0.07,
						"min": 0,
						"max": 0.2,
						"resolution": 3,
						"resetvalue": 0.07,
						"resetcaption": "Réinitialisation"
					}
				},
				{
					"type": "slider",
					"settings": {
						"title": "Ki",
						"variable": "datasources[\"ws\"][\"Ki\"]",
						"formula": "x",
						"initialvalue": 0.5,
						"min": 0,
						"max": 1,
						"resolution": 2,
						"resetvalue": 0.5,
						"resetcaption": "Réinitialisation"
					}
				},
				{
					"type": "slider",
					"settings": {
						"title": "Kd",
						"variable": "datasources[\"ws\"][\"Kd\"]",
						"formula": "x",
						"initialvalue": 0,
						"min": 0,
						"max": 0.01,
						"resolution": 4,
						"resetvalue": 0,
						"resetcaption": "Réinitialisation"
					}
				}
			]
		},
		{
			"title": "Moniteur",
			"width": 1,
			"row": {
				"3": 25,
				"4": 9,
				"5": 23
			},
			"col": {
				"3": 2,
				"4": 1,
				"5": 1
			},
			"col_width": "1",
			"widgets": [
				{
					"type": "textarea",
					"settings": {
						"nblines_window": "5",
						"value": [
							"datasources[\"ws\"][\"Raw\"]"
						]
					}
				}
			]
		}
	],
	"datasources": [
		{
			"name": "ws",
			"type": "websocket",
			"settings": {
				"host": "ws://192.168.100.199:9090/ws",
				"variables_to_send": "typeSignal,offset,amplitude,frequence,Kp,Ki,Kd",
				"refresh_rate": 500,
				"checksum": "none"
			}
		}
	],
	"columns": 4
}